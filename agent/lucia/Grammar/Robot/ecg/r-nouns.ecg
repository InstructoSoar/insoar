//  Objects
construction FORK  //  For 001
   subcase of CommonNoun
   form
      constraints
         self.f.orth <-- "fork"
   meaning: Fork

construction DRAWER  //  For 003
   subcase of CommonNoun
   form
      constraints
         self.f.orth <-- "drawer"
   meaning: Drawer

construction MUG  //  For 004
   subcase of CommonNoun
   form
      constraints
         self.f.orth <-- "mug"
   meaning: Mug

construction FRIDGE  //  For 004
   subcase of CommonNoun
   form
      constraints
         self.f.orth <-- "fridge"
   meaning: Fridge

construction DEGREES  //  For 014
   subcase of UnitsName
   form
      constraints
         self.f.orth <-- "degrees"
   meaning
      constraints
         self.m.type <-- "angle"
         self.m.concept-handle <-- "degrees"

construction APPLE   //  For 021
   subcase of CommonNoun
   form
      constraints
         self.f.orth <-- "apple"
   meaning: Apple
