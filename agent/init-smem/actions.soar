   (<parg0021> ^arg-type object ^required true ^id <obj-slot0021>)
   (<parg0022> ^arg-type partial-predicate ^id <pred-slot0022> ^2 <obj-slot0022>)
   (<graph002> ^first <goal0021> ^goal <goal0021> ^goal <goal0022>)

      # If there is no arg2, then the goal is for the object to not be grabbed
      (<goal0021> ^handle put-down1goal1 ^item-type task-goal ^conditions <gconds0021> ^predicates <gpreds0021> ^next <goal0022>)
         (<gconds0021> ^pred-count 1 ^1 <gcond002g1p1>)
