               (<pred-slot0023> ^default <ps0023def>)
                  (<ps0023def> ^predicate-handle not-grabbed1 ^property-handle is-grabbed1)

      # If there is an arg2, then the goal is to satisfy the partial predicate 
      (<goal0022> ^handle put-down1goal2 ^item-type task-goal ^conditions <gconds0022> ^predicates <gpreds0022> ^next complete)
         (<gconds0022> ^pred-count 1 ^1 <gcond002g2p1>)
            (<gcond002g2p1> ^type has-argument ^argument-name arg2)
         (<gpreds0022> ^pred-count 1 ^1 <gpred002g2p1>)
