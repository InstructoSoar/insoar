smem --add {
	(<lti> ^model <cordModel>)
	(<cordModel> ^model-name plug
 	^participants <partic>
	^preconditions <preCon>
	^causes <causes>
	^effects <effects>) 
#participants: two objects and a chord
(<partic> ^participant <par1>)
(<par1> ^handle lightswitch1
	    ^category lightswitch1)

(<partic> ^participant <par2>)
(<par2> ^handle lamp1
		^category lamp1)

#pre-condition (non)
(<preCon> ^none none)

#cause
(<causes> ^cause <cs2>)
(<cs2> ^id 1
		^task-operator <2tsk>
		^object <2obj1>
		^object <2obj2>)
(<2obj1> ^participant <par1>)
(<2obj2> ^participant <par2>)
(<2tsk>	^name op_plug1
		^item-type task-operator
		^task-handle plug1
		^arg1 <2arg1>
		^arg2 <2arg2>)
(<2arg1> ^arg-type object
		^temp-id <par1>
		^reference-info <ref>)
(<ref> ^category lightswitch1)
(<2arg2> ^arg-type partial-predicate
		^handle in1
		^temp-2 <par2>
		^reference-info <ref2>)
(<ref2> ^category lamp1)
#effect
(<effects> ^effect <ef1>)
(<ef1> ^id 1
	^model-reference <cordModel>
	^participant <par1>
	^participant <par2>
    ^pred-name connected1
    ^pred-value <par1>
	^pred-value <par2>
	^participant-category lamp1
	^participant-category lightswitch1)

	#light switch is connected to lights
}
