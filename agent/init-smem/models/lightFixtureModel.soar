smem --add {
(<lti2> ^model <lightModel>)
(<lightModel> ^model-name light-fixture
 	^participants <partic>
	^preconditions <preCon>
	^causes <causes>
	^effects <effects>)
#participants
(<partic> ^participant <par1>)
(<par1> ^handle lightswitch1
		^category lightswitch1)

(<partic> ^participant <par2>)
(<par2> ^handle lamp1
		^category lamp1)

#preconditons 
(<preCon> ^precondition <pre1>)
(<pre1> ^predicates <predsPre>
	    ^object1 <par1>
	    ^object2 <par2>
	    ^type relation)
(<predsPre> ^predicate <predPre>)
(<predPre> ^handle connected1
			^instance <instP>)
(<instP> ^1.grounded <par1>
	     ^2.grounded <par2>)
#causes
(<causes> ^cause <cs2>)
(<cs2>  ^task-operator <2tsk>
		^object <2obj1>)
(<2obj1> ^participant <par1>)
(<2tsk>	^name op_flip-on1
		^item-type task-operator
		^task-handle flip-on1
		^perform multiple
		^arg1 <2arg1>)
(<2arg1> ^arg-type object
		^id <par1>)

#effects
(<effects> ^effect <ef1>)
(<ef1> ^id 1
	 ^model-reference <lightModel>
 	 ^participant-category lamp1
 	 ^participant <par2>
	 ^pred-name activation1
	 ^pred-value on2)	
}
















