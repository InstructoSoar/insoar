
sp {interpret-message*propose*goal-feature
   (state <s> ^name interpret-message
   	      ^instructor-message <msg>
	      -^new-segment-info)
   (<msg> ^type goal-feature)
-->
   (<s> ^operator <o> +) 
   (<o> ^name goal-feature
   	^message <msg>)
}
sp {interpret-message*apply*goal-feature*object
   (state <s> ^name interpret-message
              ^operator <o>)
   (<o> ^name goal-feature
        ^message.information <info>)
   (<info> ^word <attig> ^object <objb>)
   (<objb> -^plural true)
-->
   (<s> ^new-segment-info <info>)
   (<info> ^purpose <p>
           ^originator mentor)
   (<p> ^type goal-feature-modify
        ^parameters <params>
        ^satisfaction <sat>)
   (<params> ^type <attig>
             ^indexable <i>)
   (<i> ^type object
        ^lexical <objb>
        ^index-info <idx-info>)
   (<idx-info> ^use-interaction true
               ^return-all false
               ^infer-match true)
   (<sat> ^dialog-event.type game)
}
