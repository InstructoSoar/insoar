sp {interpret-message*apply*goal-feature*relation
   (state <s> ^name interpret-message
              ^operator <o>)
   (<o> ^name goal-feature
        ^message.information <info>)
   (<info> ^word <attig> ^relation <prep>)
-->
   (<s> ^new-segment-info <info>)
   (<info> ^purpose <p>
           ^originator mentor)
   (<p> ^type goal-feature-modify
        ^parameters <params>
        ^satisfaction <sat>)
   (<params> ^type <attig>
             ^relation <prep>
             ^indexable <i2>)
   (<i2> ^type preposition
        ^lexical <prep>
        ^index-info <idx-info2>)
   (<idx-info2> ^use-interaction true
               ^return-all false
               ^infer-match false)
   (<sat> ^dialog-event.type game)
}
