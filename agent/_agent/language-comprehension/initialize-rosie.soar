sp {apply*initialize-rosie*initialize-world*tower-of-hanoi22asdf
   (state <s> ^operator.name satisfy-purposed
             -^switch
              ^world.predicates.predicate <on1>
              ^world.objects.object <loc2>
              ^world.predicates.predicate <below1>
              ^dialog-context.initial)
      (<on1> ^handle on1 
          ^instance <on-2>)
   (<on-2> ^1 <obj1> ^2 <loc>)
    (<below1> ^handle below1 
          ^instance <b-2>)
   (<b-2> ^2 <obj1> ^1 <loc>)
   (<loc> ^handle b1 ^item-type object ^predicates <ploc>)
   (<ploc> ^category location ^color green1 ^visibility visible)
   (<loc2> ^handle b3 ^item-type object ^predicates <ploc2>)
   (<ploc2> ^category location ^color blue1 ^visibility visible)
-->
   (<s> ^switch true)
   (<on1> ^instance <on-2> -)
   (<on1> ^instance <on-2a>)
   (<below1> ^instance <b-2> -)
   (<below1> ^instance <b-2a>)
   (<on-2a> ^1 <obj1> ^2 <loc2>)
   (<b-2a> ^2 <obj1> ^1 <loc2>)
}
   
