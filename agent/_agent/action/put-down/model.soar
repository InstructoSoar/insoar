sp {apply*put-down*action-model
   (state <s> -^problemspace.action.type << execute none >>
   	      ^operator <op>
	      ^environment <env>)
   (<env> ^not-predicate <npr>)
   (<op> ^name op_put-down-object-location
   	 ^argument1 <obj>
	 ^argument2 <nval>)
   (<nval> ^argument <loc>
	   ^relation <rel>) 
    (<npr> ^relation <rel>
   	   ^a <obj>
	   ^b <loc>)
-->
   (<env> ^predicate <pr>
	  ^not-predicate <npr> -)
   (<pr> ^relation <rel>
   	 ^a <obj>
	 ^b <loc>)
}

sp {apply*put-down*action-model*grabbed
   (state <s> -^problemspace.action.type << execute none >>
   	      ^operator <op>
	      ^environment <env>)
   (<env> ^grabbed <obj>)
   (<op> ^name op_put-down-object-location
   	 ^argument1 <obj>)
-->
   (<env> ^grabbed <obj> -)
}

