sp {index-verb*retrieve-lti*goal
   (state <s> ^name index-verb
              ^map-id.goal <gid>
             -^retrieved-lti <gid>)
-->
   (<s> ^nretrieve-lti <gid>)
}

sp {index-verb*unknown-goal*propose*extract-state
   (state <s> ^name index-verb
              ^topstate.verb-parameters.infer-goals true
              ^map-id.goal <gid>
              ^retrieved-lti <gid>
             -^initiated-goal-learning)
   (<gid> -^<pred>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name extract-state)
}

sp {index-verb*unknown-goal*apply*extract-state
   (state <s> ^name index-verb
              ^operator.name extract-state
              ^state-result <res>
              ^topstate.interaction.stack.segment.info <info>)
-->
   (<s> ^initiated-goal-learning true)
   (<info> ^learn-goal <lg>)
   (<lg> ^unchanged-state <res>
         ^used-objects <objs>
         ^used-relations <rels>
         ^used-actions <actions>
         ^used-predicates <preds>)
}