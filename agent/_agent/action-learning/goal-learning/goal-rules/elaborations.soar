### elaborate problemspace
sp {elaborate*rules*problemspace
   (state <s> ^name goal-rules
   	      ^problemspace <ps>)
-->
   (<ps> ^state <st>
   	 ^action <act>)
   (<st> ^dont-copy environment
	 ^dont-copy io
	 ^dont-copy object
	 ^type hypothetical)
   (<s> ^duplicates <dup>
   	^name action)
   (<act> ^type none)
}


sp {copy*indexed-action*from*segment*goal-rules
   (state <s> ^name goal-rules
   	      ^topstate.interaction.stack.segment <seg>)
   (<seg> ^purpose.type goal-description
          ^next.purpose.parameters.indexable.result <iop>)
-->
   (<s> ^topstate-indexed-action <iop>
   	^indexed-action <ia>)
}

sp {copy*indexed-action*from*segment*goal-rules*action-command
   (state <s> ^name goal-rules
   	      ^topstate.interaction.stack.segment.purpose <p>)
   (<p> ^type action-command
        ^parameters.indexable.result <iop>)
-->
   (<s> ^topstate-indexed-action <iop>
   	^indexed-action <ia>)
}

sp {copy*object*smem-rep*goal-rules
   (state <s> ^name goal-rules
   	      ^duplicates <dup>
	      ^superstate.object <obj>
	      ^object <nobj>)
   (<dup> ^<obj> <nobj>)
   (<obj> ^smem-rep <id>)
-->
   (<nobj> ^smem-rep <id>)
}