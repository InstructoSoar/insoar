### elaborate problemspace
sp {elaborate*rules*problemspace
   (state <s> ^name goal-rules
   	      ^problemspace <ps>)
-->
   (<ps> ^state <st>
   	 ^action <act>)
   (<st> ^dont-copy environment
	 ^dont-copy io
	 ^dont-copy object
	 ^type hypothetical)
   (<s> ^duplicates <dup>
   	^name action)
   (<act> ^type none)
}

sp {goal-rules*elaborate*from*superoperator
   (state <s> ^name goal-rules
              ^superstate.operator <ssop>)
   (<ssop> ^indexed-action <ia>)
-->
   (<s> ^topstate-indexed-action <ia>)
}

sp {goal-rules*elaborate*indexed-action*new
   (state <s> ^name goal-rules)
-->
   (<s> ^indexed-action <ia>)
}

sp {copy*object*smem-rep*goal-rules
   (state <s> ^name goal-rules
   	      ^duplicates <dup>
	      ^superstate.object <obj>
	      ^object <nobj>)
   (<dup> ^<obj> <nobj>)
   (<obj> ^smem-rep <id>)
-->
   (<nobj> ^smem-rep <id>)
}