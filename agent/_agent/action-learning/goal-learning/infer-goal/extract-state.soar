sp {infer-goal*propose*extract-state
   (state <s> ^name infer-goal
             -^final-state)
-->
   (<s> ^operator <o> +)
   (<o> ^name extract-state)
}

sp {infer-goal*apply*extract-state*copy-result
   (state <s> ^name infer-goal
              ^state-result <res>
              ^operator.name extract-state)
-->
   (<s> ^final-state <res>)
}