sp {infer-goal*elaborate*initial-state
   (state <s> ^name infer-goal
              ^topstate.interaction.stack.segment.info.initial-state <is>)
-->
   (<s> ^initial-state <is>)
}

sp {infer-goal*elaborate*used-items
   (state <s> ^name infer-goal
              ^topstate.interaction.stack.segment.info <info>)
   (<info> ^used-objects <objs>
           ^used-relations <rels>
           ^used-actions <actions>)
-->
   (<s> ^used-objects <objs>
        ^used-relations <rels>
        ^used-actions <actions>)
}

   