sp {infer-goal*elaborate*unchanged-state
   (state <s> ^name infer-goal
              ^topstate.interaction.stack.segment.info.learn-goal.unchanged-state <is>)
-->
   (<s> ^unchanged-state <is>)
}

sp {infer-goal*elaborate*used-items
   (state <s> ^name infer-goal
              ^topstate.interaction.stack.segment.info.learn-goal <lg>)
   (<lg> ^used-objects <objs>
         ^used-relations <rels>
         ^used-actions <actions>
         ^used-predicates <preds>)
-->
   (<s> ^used-objects <objs>
        ^used-relations <rels>
        ^used-actions <actions>
        ^used-predicates <preds>)
}

sp {infer-goal*elaborate*smem-retrieve*goal
   (state <s> ^name infer-goal
             -^retrieved-lti <gid>
              ^topstate.interaction.stack.segment.purpose.parameters.indexable.result <ia>)
   (<ia> ^_metadata.map-id.goal <gid>)
-->
   (<s> ^nretrieve-lti <gid>)
}
