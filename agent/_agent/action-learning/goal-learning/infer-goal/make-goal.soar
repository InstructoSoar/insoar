sp {infer-goal*propose*make-goal
   (state <s> ^name infer-goal)
-->
   (<s> ^operator <o> + >)
   (<o> ^name make-goal)
}

sp {infer-goal*apply*make-goal
   (state <s> ^name infer-goal
              ^topstate <top>
              ^superstate <ss>)
   (<top> ^object <obj1>
          ^object <obj2>
          ^found-prep <prep>)
   (<obj1> ^id 11)
   (<obj2> ^id 2)
   (<prep> ^type on)
-->
   (<ss> ^goal-description <goal>)
   (<goal> ^relation-predicate <rel-pred>)
   (<rel-pred> ^p1 <obj1> 
               ^p2 <obj2>
               ^relation <prep>)
}

              