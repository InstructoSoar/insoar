sp {infer-goal*propose*make-goal
   (state <s> ^name infer-goal)
-->
   (<s> ^operator <o> + >)
   (<o> ^name make-goal)
}

sp {infer-goal*apply*make-goal
   (state <s> ^name infer-goal
              ^goal-id <gid>
              ^procedural-id <pid>
              ^topstate.found-prep <prep>
              ^topstate.smem.command <cmd>
              ^superstate <ss>)
   (<pid> ^{<arg> <> <arg2> <> op_name} <obj1>
          ^<arg2>.argument <obj2>)
   (<prep> ^type on
           ^smem-rep <smem-prep>)
-->
   (<gid> ^relation-predicate <rel-pred>)
   (<rel-pred> ^identifier move-goal
               ^p1 <obj1>
               ^p2 <obj2>
               ^relation <rel>)
   (<rel> ^default <smem-prep>)
   (<cmd> ^store <gid> <rel-pred> <rel>)
   (<ss> ^learn-goal true)
}

              