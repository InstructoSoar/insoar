sp {propose*turn-on-light-finished
(state <s> ^type state
		^name EMA
		^world <world>)
(<world> ^objects.object <objWorld>)
(<objWorld> ^predicates.activation on
		 ^root-category lamp)
-->
(<s> ^operator <o> + >)
(<o> ^name turn-on-light-finished)
}

sp {apply*turn-on-light-finished
(state <s> ^type state
		^name EMA
		^world <world>
		^operator <o>
		^goal <glOther>)
(<o> ^name turn-on-light-finished)
(<glOther> ^task.finished false)
-->
(<glOther> ^task.finished true
	 ^task.finished false -)
(write |ALL DONE|)
}
