sp {apply*openDoor
(state <s> ^type state
		^name EMA
		^world <wrd>
		^operator <o>)
(<wrd> ^objects.object <doorObj>)
(<o> ^name open-door
	^door <doorObj>)
(<doorObj> ^root-category door
		^predicates <preds>)
(<preds> ^state closed)
-->
(<doorObj> ^state open
			^state closed -)
}

sp {apply*pushButton
(state <s> ^type state
		^operator <o>
		^world <wrd>)
(<wrd> ^objects.object <buttonObj>)
(<o>	^name op_push_button
		^item-type task-operator
		^task-handle push_button
		^perform multiple
		^arg1 <arg1>)
(<arg1> ^arg-type object
		^id <buttonObj>)
(<buttonObj> ^predicates <preds>)
(<preds> ^activation off
		^category button)
-->
(<preds> ^activation on
		 ^activation off -)
}


sp {apply*flipSwitch
(state <s> ^type state
		^operator <o>
		^world <wrd>)
(<wrd> ^objects.object <lightSwitchObj>)
(<o>	^name op_flip_switch
		^item-type task-operator
		^task-handle flip_switch
		^perform multiple
		^arg1 <arg1>)
(<arg1> ^arg-type object
		^id <lightSwitchObj>)
(<lightSwitchObj> ^predicates <preds>)
(<preds> ^activation off
		^category switch)
-->
(<preds> ^activation on
		 ^activation off -)
}

sp {apply*plug-in
(state <s> ^type state
		^world <wrd>
		^operator <o>)
(<wrd> ^objects <objs>
	   ^predicates <preds>)
(<objs> ^object <lightSwitchObj> <lightFixtureObj>)
(<lightSwitchObj> ^predicates.category switch)
(<lightFixtureObj> ^predicates.category lamp)
(<o>	^name op_plug_in
		^item-type task-operator
		^task-handle plug_in
		^perform multiple
		^arg1 <arg1>
		^arg2 <arg2>)
(<arg1> ^arg-type object
		^id <lightSwitchObj>)
(<arg2> ^arg-type object
		^id <lightFixtureObj>)
-->
(<preds> ^predicate <pred1>)
(<s> ^operator <o> -)
(<pred1> ^name plugged-in 
		 ^instance <in>)
(<in> ^1 <lightSwitchObj> 
	  ^2 <lightFixtureObj>)
}
