#read in command from the input link

#turn on light switch
sp {propose*prepare-room
(state <s> ^type state
		^name EMA
		^iox.input-link <iox>)
(<iox> ^goal.task <ts>)
(<ts> ^name prepare-room
	^finished false)
-->
(<s> ^operator <o> +)
(<o> ^name prepare-room)
}

sp {apply*prepare-room
(state <s> ^type state
		^name EMA
		^operator <o>
		^world.objects.object <obj>
		^iox.input-link <iox>)
(<iox> ^goal.task <ts>)
(<ts> ^name prepare-room
	^finished false)
(<obj> ^handle lightswitch
		^predicates <N3>)
(<N3> ^activation off)
(<o> ^name prepare-room)
-->
(<N3> ^activation on
	 ^activation off -)
(<ts> ^finished true
	^finished false -)
}