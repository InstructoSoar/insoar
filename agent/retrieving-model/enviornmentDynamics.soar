sp {elaborate*turn-on-light
(state <s> ^type state
		^world <world>
		^superstate nil)
(<world> ^objects <objs>
		^predicates <pred>)
(<objs> ^object <lighSwitObj> <lightFixtureObj>)
(<lighSwitObj> ^predicates <N3>)
(<N3> ^activation on
	 ^category trigger)
(<lightFixtureObj> ^predicates <N11>)
(<N11> ^category lightFixture)
(<pred> ^predicate <pred1>)
(<pred1> ^name connected ^instance <in>)
(<in> ^1 <lighSwitObj> ^2 <lightFixtureObj>)
-->
(<N11> ^activation on
		^activation off -)
}

sp {elaborate*connected
(state <s> ^type state
		^world <world>
		^superstate nil)
(<world> ^objects <objs>
		^predicates <pred>)
(<objs> ^object <lighSwitObj> <lightFixtureObj>)
(<lightFixtureObj> ^handle lightFixture)
(<pred> ^predicate <pred1>)
(<pred1> ^name plugged-in 
		 ^instance <in>)
(<in> ^1 <lighSwitObj> 
	  ^2 <lightFixtureObj>)
-->
(<pred> ^predicate <pred2>)
(<pred2> ^name connected 
	      ^instance <in2>)
(<in2> ^1 <lighSwitObj> 
	   ^2 <lightFixtureObj>)

}